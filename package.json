{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "^5.9.3",
    "@react-navigation/native": "^5.7.6",
    "axios": "^0.20.0",
    "eslint": "^7.11.0",
    "expo": "~39.0.2",
    "expo-status-bar": "~1.0.2",
    "lottie-react-native": "^3.5.0",
    "moment": "^2.29.1",
    "native-base": "^2.13.14",
    "node-fetch": "^2.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-animated-loader": "0.0.8",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-screens": "^2.11.0",
    "react-native-timeago": "^0.5.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "^0.13.18",
    "react-navigation": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.1",
    "jest-environment-jsdom": "^26.6.1",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^39.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^16.4.0",
    "mockdate": "^3.0.2",
    "prettier": "^2.1.2",
    "react-native-custom-tabs": "^0.1.7",
    "react-native-device-info": "^7.0.2",
    "react-native-mock-render": "^0.1.9",
    "react-native-safari-view": "^2.1.0",
    "react-test-renderer": "^17.0.1",
    "ts-node": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "verbose": true,
    "testURL": "http://localhost/",
    "preset": "jest-expo",
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "setupFiles": [
      "./setupJest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/(?!react-native|react-clone-referenced-element|react-navigation)"
    ]
  },
  "private": true,
  "bin": {
    "gh-viewer": "node /src/cli.js"
  }
}
